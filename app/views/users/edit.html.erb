<h1>Update user </h1>

<%= form_for @user do |f| %>
    <%= render partial: 'error' %>

    <%= f.label :username, "Username" %>
    <%= f.text_field :username %>
    <%= f.label :email, "email" %>
    <%= f.email_field :email %>
    <%= f.label :password, "password" %>
    <%= f.password_field :password %>
    <%= f.submit 'Register'%>
<% end %>